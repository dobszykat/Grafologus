<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

  <item android:state_pressed="true">
    <shape>
      <solid
          android:color="#C3A466" />
      <stroke
          android:width="3dp"
          android:color="#120F26" />
      <corners
          android:radius="25dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
    <item android:state_enabled="false">
    <shape>
      <solid
          android:color="#99ABB6" />
      <stroke
          android:width="3dp"
          android:color="#2F7DAA" />
      <corners
          android:radius="25dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#716F7C"
          android:endColor="#373639"
          android:angle="270" />
      <stroke
          android:width="3dp"
          android:color="#C3A466"/>
      <corners
          android:radius="25dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>

</selector>